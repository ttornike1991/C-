//Example1

int main()
{
int i;
for (i=0; i<10; i+=1){
    printf("hi\n");
}
printf ("Hello World!");


return 0;





}



//Example2


int main()
{
int i;
int n;
char b[50];
printf("Enter Word\n");
scanf("%s",&b);
printf("Enter NUM\n");
scanf("%d",&n);


for (i=0; i<n; i+=1){
    printf ("%s\n",b);

}



return 0;





}


//Example 3


int main()
{
int i;
int b=0;

for (i=0; i<5; i+=1 ){
        printf("%d",i);
        b+=i;

}
printf("\n%d",b);




return 0;
}
