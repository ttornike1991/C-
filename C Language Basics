
int main()
{

    char b[50];
    char a[50];
    printf("Enter first Word \n");
    scanf("%s",&b);
    printf("Enter first Word \n");
    scanf("%s",&a);

    if (strcmp(a,b)==0){
        printf("Len strings are Equal");

    }
    else {
        printf("Len strings not Equal");
    }
    return 0;
}
