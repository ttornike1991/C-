int main()
{
int a,i;
int sum=0;

printf("Enter  number    ");
scanf("%d",&a);
if (a==1){
    for (i=0;i<10;i++)
    {
        sum+=i;
    }
    printf("%d  Datvala for_it",sum);
}
else if (a==0){
        while (i<10){
        sum+=i;
       i+=1;
        }
printf("%d   Datvala while_it",sum);
}
else {
    printf("UNDA SHEIYVANO 1 an 0");
}


return 0;

}
